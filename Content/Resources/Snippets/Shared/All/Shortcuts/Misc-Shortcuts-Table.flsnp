<?xml version="1.0" encoding="utf-8"?>
<html xmlns:MadCap="http://www.madcapsoftware.com/Schemas/MadCap.xsd">
    <head>
        <link href="../../../../TableStyles/Shared/Fancy.css" rel="stylesheet" MadCap:stylesheetType="table" />
        <link href="" rel="stylesheet" type="text/css" />
    </head>
    <body>
        <table style="width: 100%;margin-left: 0;margin-right: auto;caption-side: top;mc-table-style: url('../../../../TableStyles/Shared/Fancy.css');" class="TableStyle-Fancy" cellspacing="0">
            <col class="TableStyle-Fancy-Column-Regular" style="width: 50%;" />
            <col class="TableStyle-Fancy-Column-Regular" style="width: 50%;" />
            <thead>
                <tr class="TableStyle-Fancy-Head-Header1" MadCap:conditions="General.Print-Only">
                    <th class="TableStyle-Fancy-HeadD-Regular-Header1" colspan="2" MadCap:conditions="">
                        <p class="TableTextHead">Miscellaneous</p>
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr valign="top" class="TableStyle-Fancy-Body-LightRows" MadCap:conditions="">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows">
                        <p class="TableTextEmphasis">Close Application</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows">
                        <p class="TableText">ALT+F4</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Products.Capture,Snippets.Snippet5" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Decrease Eraser Size</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p>[</p>
                    </td>
                </tr>
                <tr valign="top" class="TableStyle-Fancy-Body-LightRows" MadCap:conditions="Snippets.Snippet1">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows">
                        <p class="TableTextEmphasis">Decrease Internal Text Editor Font Size</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows">
                        <p class="TableText">CTRL+-</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Products.Capture,Snippets.Snippet5" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Decrease Pencil Size</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p>[</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Products.Capture,Snippets.Snippet5" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Increase Eraser Size</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p>]</p>
                    </td>
                </tr>
                <tr valign="top" class="TableStyle-Fancy-Body-DarkerRows" MadCap:conditions="Snippets.Snippet1">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows">
                        <p class="TableTextEmphasis">Increase Internal Text Editor Font Size</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows">
                        <p class="TableText">CTRL+=</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Products.Capture,Snippets.Snippet5" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Increase Pencil Size</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p>]</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Products.Capture,Products.Mimic" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Hand Mode</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableText">SPACE</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet1,Snippets.Snippet2,Snippets.Snippet3,Snippets.Snippet6" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Move Block Elements and Table Rows Down or Up</p>
                        <p class="TableText" MadCap:conditions="General.Online-Only">
                            <MadCap:snippetText src="Block-Items-Move.flsnp" />
                        </p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p class="TableText">ALT+SHIFT+DOWN</p>
                        <p class="TableText">ALT+SHIFT+UP</p>
                    </td>
                </tr>
                <tr valign="top" class="TableStyle-Fancy-Body-DarkerRows" MadCap:conditions="Snippets.Snippet4">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Open Quick Find Widget</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableText">CTRL+F</p>
                    </td>
                </tr>
                <tr valign="top" class="TableStyle-Fancy-Body-LightRows" MadCap:conditions="Products.Mimic">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Preview Mode</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p class="TableText">CTRL+SHIFT</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet1,Snippets.Snippet3,Snippets.Snippet5,Snippets.Snippet6,Snippets.Snippet8" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Print</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableText">CTRL+P</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet1,Snippets.Snippet2,Snippets.Snippet3,Snippets.Snippet4,Snippets.Snippet5,Snippets.Snippet7,Snippets.Snippet6,Snippets.Snippet9,Snippets.Snippet10" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Quick Access Buttons</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p class="TableText">ALT+1-9</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet1" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Record Macro</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableText">CTRL+ALT+;</p>
                    </td>
                </tr>
                <tr valign="top" class="TableStyle-Fancy-Body-LightRows" MadCap:conditions="">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows">
                        <p class="TableTextEmphasis">Refresh</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows">
                        <p class="TableText">F5</p>
                    </td>
                </tr>
                <tr valign="top" class="TableStyle-Fancy-Body-DarkerRows" MadCap:conditions="Snippets.Snippet1">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows">
                        <p class="TableTextEmphasis">Reset Internal Text Editor Font Size</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows">
                        <p class="TableText">CTRL+0</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet5,Snippets.Snippet8" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Select All</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p class="TableText">CTRL+A</p>
                    </td>
                </tr>
                <tr valign="top" class="TableStyle-Fancy-Body-DarkerRows" MadCap:conditions="">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows">
                        <p class="TableTextEmphasis">Tab Between User Interface Elements</p>
                        <p MadCap:conditions="General.Online-Only">
                            <MadCap:snippetText src="Tab-Between-UI-Elements.flsnp" />
                        </p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows">
                        <p class="TableText">CTRL+TAB</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet1,Snippets.Snippet3" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Track Changes</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p class="TableText">CTRL+SHIFT+E</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet4" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Upload Translation (File) to Database</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableText">ALT+SHIFT+U</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet4" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Upload Translation (Selection) to Database</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p class="TableText">ALT+U</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet5,Snippets.Snippet8" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Zoom 100%</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableText">CTRL+0</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet5,Snippets.Snippet8" class="TableStyle-Fancy-Body-LightRows">
                    <td class="TableStyle-Fancy-BodyE-Regular-LightRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Zoom In</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyD-Regular-LightRows" MadCap:conditions="">
                        <p class="TableText">CTRL++</p>
                    </td>
                </tr>
                <tr valign="top" MadCap:conditions="Snippets.Snippet5,Snippets.Snippet8" class="TableStyle-Fancy-Body-DarkerRows">
                    <td class="TableStyle-Fancy-BodyB-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableTextEmphasis">Zoom Out</p>
                    </td>
                    <td class="TableStyle-Fancy-BodyA-Regular-DarkerRows" MadCap:conditions="">
                        <p class="TableText">CTRL+-</p>
                    </td>
                </tr>
            </tbody>
        </table>
    </body>
</html>