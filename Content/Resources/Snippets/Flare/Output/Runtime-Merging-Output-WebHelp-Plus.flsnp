        <MadCap:snippetBlock src="WHP/Scenario-Link.flsnp" MadCap:conditions="General.Online-Only" MadCap:conditionTagExpression="include[Snippets.Snippet9], exclude[Snippets.Snippet10]" />
        <table style="width: 100%;margin-left: 0;margin-right: auto;mc-table-style: url('../../../TableStyles/Shared/Fancy.css');" class="TableStyle-Fancy" cellspacing="0">
