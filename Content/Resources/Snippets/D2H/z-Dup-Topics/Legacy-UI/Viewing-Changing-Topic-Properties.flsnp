                            <MadCap:dropDownHead>
                                <MadCap:dropDownHotspot>General</MadCap:dropDownHotspot>
                            </MadCap:dropDownHead>
                            <MadCap:dropDownBody>
                                <ul>
                                    <li>
                                        <MadCap:snippetText src="../../Topics/Topic-Title.flsnp" />
                                    </li>
                                    <li>
